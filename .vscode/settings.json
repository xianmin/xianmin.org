{
  "editor.fontSize": 20,
  "editor.fontFamily": "'Noto Sans Mono CJK SC'",
  "editor.lineHeight": 1.682,
  "editor.lineNumbers": "off",
  "todo-tree.highlights.customHighlight": {
    "[ ]": {
      "background": "#ff000080"
    },
    "[x]": {
      "background": "#00ff0080"
    }
  },
  "git.enableSmartCommit": true,
  "git.postCommitCommand": "sync",
  "git.confirmSync": false
}
